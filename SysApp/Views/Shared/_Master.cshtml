
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>System</title>

    @Styles.Render("~/Master/css")
    
</head>

<body>
    @Scripts.Render("~/Master/js")
    <section id="container">
        <!-- **********************************************************************************************************************************************************
            TOP BAR CONTENT & NOTIFICATIONS
            *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a class="logo"><b>SYSTEM<span> APP</span></b></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <!--  notification end -->
            </div>
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="/">Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
            MAIN SIDEBAR MENU
            *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse " style="background:#00583f" !important>
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <p class="centered"><a><img src="~/lib/img/ui-admin.png" class="img-circle" width="80"></a></p>
                    <!-- <h5 class="centered">Rachell Cordero</h5> -->
                    <h5 class="centered">@(Model.CurrentUser.FirstName + " " + Model.CurrentUser.LastName)</h5>
                    <div style="display:none" id="userID">@Model.CurrentUser.AccountID</div>
                    <div style="display:none" id="role">@Model.CurrentUser.Role</div>

                    <li class="mt">
                        <a href="/home/Accounts?id=@Model.CurrentUser.UID" id="a_users">
                            <i class="fa fa-users"></i>
                            <span>Users</span>
                        </a>
                    </li>
                    <li>
                        <a href="/home/role?id=@Model.CurrentUser.UID" id="a_roles">
                            <i class="fa fa-wrench"></i>
                            <span>Roles</span>
                        </a>
                    </li>
                    <li>
                        <a href="/home/Permission?id=@Model.CurrentUser.UID" id="a_permission">
                            <i class="fa fa-lock"></i>
                            <span>Permissions</span>
                        </a>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!-- **********************************************************************************************************************************************************
            MAIN CONTENT
            *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
                <!-- /row -->
            </section>
        </section>
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer" style="background:#00583f" !important>
            <div class="text-center">

                <div class="credits">

                    Created by Rachell Cordero
                </div>
                <a class="go-top">
                    <i class="fa fa-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>

    <script type="text/javascript">
        $(document).ready(function () {


            return false;
        });
    </script>
</body>

</html>
